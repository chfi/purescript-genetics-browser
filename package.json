{
  "name": "purescript-genetics-browser",
  "version": "0.0.1",
  "description": "Genetics network browser written in Purescript",
  "dependencies": {
    "cytoscape": "^3.1.1",
    "ipfs-api": "^14.1.2"
  },
  "devDependencies": {
    "bower": "1.8.0",
    "pulp": "11.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chfi/purescript-genetics-browser.git"
  },
  "scripts": {
    "deps": "bower install",
    "build": "pulp build",
    "build-to": "pulp browserify --main Genetics.Browser.UI.Container --skip-entry-point --standalone PS --to",
    "watch-to": "pulp --watch browserify --main Genetics.Browser.UI.Container --skip-entry-point --standalone PS --to",
    "test": "pulp test",
    "watch-test": "pulp --watch test",
    "docs": "pulp docs"

  }
}
